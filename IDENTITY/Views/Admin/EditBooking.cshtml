@model IDENTITY.Models.BookingModel

<h2>Edit Booking @Model.ID</h2>

<form asp-action="EditBooking" method="post">
    <input type="hidden" asp-for="ID" />
    <input type="hidden" asp-for="RoomModelId" />
    <input type="hidden" asp-for="UserID" />


    <div class="form-group">
        <label asp-for="StartTime">Start Time</label>
        <input asp-for="StartTime" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="EndTime">End Time</label>
        <input asp-for="EndTime" class="form-control" />
    </div>

    <!-- Add more form fields for editing other booking properties if needed -->

    <button type="submit" class="btn btn-primary">Save Changes</button>
</form>

